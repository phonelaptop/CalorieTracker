{
  "name": "calorietracker",
  "version": "1.0.0",
  "description": "CalorieTracker - AI-powered nutrition tracking",
  "private": true,
  "scripts": {
    "install:all": "concurrently \"cd client && npm i\" \"cd server && npm i\"",
    "dev:client": "cd client && npm start",
    "dev:server": "cd server && npm start",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "build:client": "cd client && npm run build",
    "build:server": "cd server && npm run build"
  },
  "keywords": ["nutrition", "calorie-tracker", "health"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}